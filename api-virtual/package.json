{
  "name": "api-virtual",
  "version": "0.1.0",
  "private": true,
  "description": "Servidor de APIs virtuales que monta varias especificaciones OpenAPI y handlers declarativos.",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "debug": "ts-node-dev --respawn --inspect=0.0.0.0:9229 --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.23",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
